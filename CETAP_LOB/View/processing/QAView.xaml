    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Converters="clr-namespace:CETAP_LOB.Converters"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ignore="http://www.ignore.com"
    xmlns:local="clr-namespace:CETAP_LOB.View.writers"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:mui="http://firstfloorsoftware.com/ModernUI"
    HorizontalAlignment="Left"
    d:DesignHeight="700"
    d:DesignWidth="1200"
    mui:ModernFrame.KeepAlive="false"
    BorderThickness="0,1,0,0"
    mc:Ignorable="d ignore">
    <UserControl.Resources>
        <Converters:NumberToColorConverter x:Key="NumberToColorConverter" />
        <Style x:Key="TextBlockStyle" TargetType="{x:Type TextBlock}">
            <Style.Triggers>
                <Trigger Property="Validation.HasError" Value="True">
                    <Setter Property="ToolTip" Value="{Binding (Validation.Errors)[0].ErrorContent, RelativeSource={RelativeSource self}}" />
                </Trigger>
            </Style.Triggers>
        </Style>

    </UserControl.Resources>
    <UserControl.BorderBrush>
        <SolidColorBrush Color="{Binding SettingsAppearance.SelectedAccentColor, Source={StaticResource Locator}}" />
    </UserControl.BorderBrush>
    <UserControl.DataContext>
        <Binding Path="QA" Source="{StaticResource Locator}" />
    </UserControl.DataContext>

